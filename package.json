{
  "name": "eui-component-template",
  "version": "0.1.4",
  "description": "EUI component template",
  "main": "dist/index.js",
  "scripts": {
    "start": "eui server",
    "dev": "eui server",
    "build": "eui build",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint ./src & eslint ./demo & eslint ./__test__ & eslint ./__mocks__",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "version": "npm run changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run build"
  },
  "author": "mistong eui team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MST-EUI/EUI-component-tpl.git"
  },
  "bugs": {
    "url": "https://github.com/MST-EUI/EUI-component-tpl/issues"
  },
  "homepage": "https://github.com/MST-EUI/EUI-component-tpl",
  "keywords": [
    "react",
    "react-component",
    "eui"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@mistong/eui-cli": "^0.1.6",
    "@mistong/eui-css": "^0.1.2",
    "babel-jest": "^23.2.0",
    "conventional-changelog-cli": "^2.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "es5-shim": "^4.5.10",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "jest": "^23.2.0",
    "react": "^0.14.9",
    "react-dom": "^0.14.9"
  },
  "dependencies": {
    "classnames": "^2.2.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        0
      ]
    }
  }
}
